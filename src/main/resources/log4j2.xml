<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
	<Appenders>
		<File name="SFTPFileSyncLog" fileName="SFTPFileSync.log"
			immediateFlush="false" append="true">
			<PatternLayout
				pattern="%d{yyy-MM-dd HH:mm:ss.SSS} [%level] %msg%n" />
		</File>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="SFTPFileSyncLog" />
		</Root>
	</Loggers>
</Configuration>